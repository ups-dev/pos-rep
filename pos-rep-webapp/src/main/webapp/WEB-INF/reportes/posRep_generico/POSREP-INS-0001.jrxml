	<queryString>
		<![CDATA[SELECT DISTINCT
       sed.est_codigo                           cod_sede,
       dse.dee_descripcion                      sede,
       dcm.dee_descripcion                      campus,
       dca.dee_descripcion                      posgrado,
       pel.pel_descripcion                      cohorte,
       cli.cllc_ruc                             nro_id,
       alu.alu_apellidos ||' '||alu.alu_nombres nombres,
       alu.alu_email                            correo_institucional,
       cli.cllc_email                           correo_personal,
       cli.cllc_fono                            telefono,
       cli.cllc_celular                         celular,
       decode(paf.paf_pagado,'S','SI','NO')     pagado,
       inc.ins_fecha_inscripcion                fecha_inscripcion,
       alu.alu_clave                            contrasenia_inicial
FROM   ins.ins_inscripcion                      inc,
       ins.ins_alumno                           alu,
       ins.ins_oferta_inscripcion_inicial       oii,
       sigac.cliente_local                      cli,
       org.org_estructura                       car,
       org.org_descripcion_estructura           dca,
       org.org_estructura                       cam,
       org.org_descripcion_estructura           dcm,
       org.org_estructura                       sed,
       org.org_descripcion_estructura           dse,
       ins.ins_tip_apr_ins                      tai,
       ins.ins_tipo_inscripcion                 tii,
       ins.ins_tipo_aprobacion                  tap,
       fac.fac_pago_factura                     paf,
       ped.ped_proyecto_educacion               ped,
       org.org_periodo_lectivo                  pel
WHERE  inc.alu_codigo                           = alu.alu_codigo
AND    inc.oii_codigo                           = oii.oii_codigo
AND    alu.cllc_cdg                             = cli.cllc_cdg
AND    car.est_codigo                           = oii.est_codigo
AND    car.dee_codigo                           = dca.dee_codigo
AND    car.est_codigo_padre                     = cam.est_codigo
AND    cam.dee_codigo                           = dcm.dee_codigo
AND    cam.est_codigo_padre                     = sed.est_codigo
AND    sed.dee_codigo                           = dse.dee_codigo
AND    car.est_codigo  IN (SELECT est_codigo e
                            FROM   org.org_estructura e
                            WHERE  e.est_nivel  = 4
                            START WITH e.est_codigo = $P{pn_est_codigo}
                            CONNECT BY PRIOR e.est_codigo = e.est_codigo_padre)
AND    inc.tai_codigo                           = tai.tai_codigo
AND    tai.tii_codigo                           = tii.tii_codigo
AND    tai.tia_codigo                           = tap.tia_codigo
AND    tai.tai_codigo                           = 17
AND    inc.ins_codigo                           = paf.ins_codigo
AND    paf.tip_codigo                           = 8 -- Inscripcion de Posgrado
AND    inc.ins_vigencia                         = 'S'
AND    inc.ins_anulado                          = 'N'
AND    inc.pre_numero                           = ped.pre_numero(+)
AND    oii.pel_codigo                           = pel.pel_codigo
AND    pel.pel_codigo                           = $P{pn_pel_codigo}
AND    paf.paf_pagado                           like $P{pv_paf_pagado}
ORDER BY  sed.est_codigo,campus,posgrado,cohorte,nombres]]>
	</queryString>
