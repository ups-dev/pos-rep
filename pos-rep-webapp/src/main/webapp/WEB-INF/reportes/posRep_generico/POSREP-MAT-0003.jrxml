	<queryString>
		<![CDATA[select sd.sed_descripcion sede,
       cm.cam_descripcion campus,
       ps.car_descripcion posgrado,
       tep.ted_descripcion cohorte,
       mp.pel_codigo,
       md.mod_descripcion modalidad,
       cli.cllc_ruc cedula,cli.cllc_cdg,alu.alu_codigo,
       cli.cllc_nmb estudiante,
       ' - ' nivel,
       mp.map_numero nro_matricula,
       to_char(mp.map_fecha_prematricula,'dd/mm/yyyy') fecha_matricula,
       pel.pel_descripcion fecha_semestre
from sna.sna_matricula_postgrado mp,
     sna.sna_modalidad           md,
     sna.sna_carrera             ps,
     sna.sna_campus              cm,
     sna.sna_sede                sd,
     sna.sna_periodo_lectivo     pel,
     sna.sna_tipo_edicion_postgrado tep,
     sigac.cliente_local         cli,
     ins.ins_alumno              alu
where mp.map_pagado = 'S'
and   mp.map_eliminado = 'N'
and   mp.map_anulado = 'N'
and   md.mod_codigo  = mp.mod_codigo_postgrado
and   ps.car_codigo = mp.car_codigo_postgrado
and   ps.fac_codigo = mp.fac_codigo_postgrado
and   cm.cam_codigo = mp.cam_codigo_postgrado
and   cm.sed_codigo = mp.sed_codigo
and   sd.sed_codigo = mp.sed_codigo
and   pel.pel_codigo = mp.pel_codigo
and   tep.ted_codigo = mp.ted_codigo
and   cli.cllc_cdg   = mp.cllc_cdg
and   cli.cllc_cdg   = alu.cllc_cdg
and   alu.alu_codigo = $P{pn_alu_codigo}


UNION

SELECT  dsed.dee_descripcion        sede,
        dcam.dee_descripcion        campus,
        dpos.dee_descripcion        posgrado,
        pel.pel_descripcion         cohorte,
        pel.pel_codigo,
        mod.mod_descripcion         modalidad,
        cli.cllc_ruc                cedula,cli.cllc_cdg,alu.alu_codigo,
        cli.cllc_nmb                estudiante,
        to_char(pee.pee_nivel)      nivel,
        mta.mat_codigo,
        to_char(mta.mat_fecha_registro,'dd/mm/yyyy')      fecha_matricula,
        extract (year from pee.pee_fecha_inicial)||' - '||extract (year from pee.pee_fecha_final) fecha_semestre
FROM  mat.mat_matricula mta,
      ped.ped_proyecto_educacion  pre,
      ped.ped_des_est_pro_edu     dep,
      org.org_descripcion_estructura dpos,
      org.org_descripcion_estructura dcam,
      org.org_descripcion_estructura dsed,
      org.org_estructura          pos,
      org.org_estructura          cam,
      org.org_estructura          sed,
      org.org_periodo_estructura  pee,
      org.org_periodo_lectivo     pel,
      ins.ins_alumno              alu,
      sigac.cliente_local         cli,
      ped.ped_est_mod_pro_edu     emp,
      ped.ped_mod_pro_edu         mpe,
      ped.ped_modalidad           mod

WHERE mta.mat_tipo_proceso = 'M'
AND   mta.mat_pagado = 'S'
AND   mta.aud_eliminado = 'N'
AND   pre.pre_numero  = dep.pre_numero
AND   dep.dep_codigo  = mta.dep_codigo
AND   dep.dee_codigo  = dpos.dee_codigo
AND   pos.dee_codigo  = dpos.dee_codigo
AND   cli.cllc_cdg    = mta.cllc_cdg
AND   cli.cllc_cdg    = alu.cllc_cdg
AND   cam.est_codigo  = pos.est_codigo_padre
AND   cam.dee_codigo  = dcam.dee_codigo
AND   sed.est_codigo  = cam.est_codigo_padre
AND   sed.dee_codigo  = dsed.dee_codigo
AND   mta.pee_codigo  = pee.pee_codigo
AND   pee.pel_codigo  = pel.pel_codigo
AND   emp.est_codigo  = pos.est_codigo_padre
AND   emp.emp_codigo  = mta.emp_codigo
AND   mpe.mpe_codigo  = emp.mpe_codigo
AND   mod.mod_codigo  = mpe.mod_codigo
AND   alu.alu_codigo  = $P{pn_alu_codigo}
ORDER   BY fecha_semestre,sede,campus,posgrado,nivel,estudiante]]>
	</queryString>
