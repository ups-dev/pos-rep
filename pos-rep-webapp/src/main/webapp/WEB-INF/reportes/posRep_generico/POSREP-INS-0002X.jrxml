	<queryString>
		<![CDATA[SELECT DISTINCT inc.ins_codigo,
       sed.est_codigo                           cod_sede,
       dse.dee_descripcion                      sede,
       dcm.dee_descripcion                      campus,
       dca.dee_descripcion                      posgrado,
       pel.pel_descripcion                      cohorte,
       cli.cllc_ruc                             nro_id,
       alu.alu_apellidos                        apellidos,
       alu.alu_nombres                          nombres,
       alu.alu_email                            correo_institucional,
       cli.cllc_email                           correo_personal,
       cli.cllc_fono                            telefono,
       cli.cllc_celular                         celular,
       NVL((select distinct 'SI'
           from sna.sna_acta_grado            ag,
                sna.sna_titulo_proyecto_academico tp
            where tp.pra_numero = ag.pra_numero
            AND tp.fac_codigo = ag.fac_codigo
            AND tp.car_codigo = ag.car_codigo
            AND tp.tit_codigo = ag.tit_codigo
            AND tp.tpa_titulo_terminal = 'S'
            and ag.acg_eliminado = 'N'
            AND ag.cllc_cdg = cli.cllc_cdg
            UNION
            select distinct 'SI'
            from ins.ins_ins_pro_aca ipa,
                 stg.stg_trabajo_graduacion tg,
                 stg.stg_acta_grado ag,
                 org.org_descripcion_estructura dca,
                 org.org_estructura             car,
                 org.org_tipo_estructura tie,
                 ins.ins_inscripcion      ins,
                 ins.ins_alumno           al
            where ipa.ipa_codigo = tg.ipa_codigo
            and  tg.tdg_codigo   = ag.tdg_codigo
            and  ag.aud_eliminado = 'N'
            and  dca.dee_codigo   = car.dee_codigo
            and  car.est_codigo   = tg.est_codigo
            and  dca.tie_codigo  = tie.tie_codigo
            and  tie.tie_codigo  = 5 --Codigo del Tipo Estructura 5-->GRADO
            and  ins.ins_codigo  = ipa.ins_codigo
            and  al.alu_codigo  = ins.alu_codigo
            and  al.cllc_cdg    = alu.cllc_cdg),'NO')graduado_ups,
        inc.ins_fecha_inscripcion                fecha_inscripcion,
        decode(paf.paf_pagado,'S','SI','NO')     pagado_insc,
        (select distinct min(mta.mat_fecha_registro)
        from mat.mat_matricula mta,
             org.org_periodo_estructura pee
        where mta.ins_codigo = inc.ins_codigo
        and  mta.mat_tipo_proceso   = 'M'
        and  mta.aud_eliminado      = 'N'
        and  mta.mat_anulado        = 'N'
        and  pee.pee_codigo = mta.pee_codigo
        and  pee.pee_nivel  = 1
        and  pee.pel_codigo = pel.pel_codigo) fecha_matricula,
        (select distinct decode(mta.mat_pagado,'S','SI','NO')
        from mat.mat_matricula mta,
             org.org_periodo_estructura pee
        where mta.ins_codigo = inc.ins_codigo
        and  mta.mat_tipo_proceso   = 'M'
        and  mta.aud_eliminado      = 'N'
        and  mta.mat_anulado        = 'N'
        and  pee.pee_codigo = mta.pee_codigo
        and  pee.pee_nivel  = 1
        and  pee.pel_codigo = pel.pel_codigo)pagado_matr,
        decode(inc.ins_confirmado,'S','SI','N','NO',null) confirmado
FROM   ins.ins_inscripcion                      inc,
       ins.ins_alumno                           alu,
       ins.ins_oferta_inscripcion_inicial       oii,
       sigac.cliente_local                      cli,
       org.org_estructura                       car,
       org.org_descripcion_estructura           dca,
       org.org_estructura                       cam,
       org.org_descripcion_estructura           dcm,
       org.org_estructura                       sed,
       org.org_descripcion_estructura           dse,
       ins.ins_tip_apr_ins                      tai,
       ins.ins_tipo_inscripcion                 tii,
       ins.ins_tipo_aprobacion                  tap,
       fac.fac_pago_factura                     paf,
       ped.ped_proyecto_educacion               ped,
       org.org_periodo_lectivo                  pel
WHERE  inc.alu_codigo                           = alu.alu_codigo
AND    inc.oii_codigo                           = oii.oii_codigo
AND    alu.cllc_cdg                             = cli.cllc_cdg
AND    car.est_codigo                           = oii.est_codigo
AND    car.dee_codigo                           = dca.dee_codigo
AND    car.est_codigo_padre                     = cam.est_codigo
AND    cam.dee_codigo                           = dcm.dee_codigo
AND    cam.est_codigo_padre                     = sed.est_codigo
AND    sed.dee_codigo                           = dse.dee_codigo
AND    car.est_codigo  IN (SELECT est_codigo e
                            FROM   org.org_estructura e
                            WHERE  e.est_nivel  = 4
                            START WITH e.est_codigo = $P{pn_est_codigo}
                            CONNECT BY PRIOR e.est_codigo = e.est_codigo_padre)
AND    inc.tai_codigo                           = tai.tai_codigo
AND    tai.tii_codigo                           = tii.tii_codigo
AND    tai.tia_codigo                           = tap.tia_codigo
AND    tai.tai_codigo                           = 17
AND    inc.ins_codigo                           = paf.ins_codigo
AND    paf.tip_codigo                           = 8 -- Inscripcion de Posgrado
AND    inc.ins_vigencia                         = 'S'
AND    inc.ins_anulado                          = 'N'
AND    inc.pre_numero                           = ped.pre_numero(+)
AND    oii.pel_codigo                           = pel.pel_codigo
AND    pel.pel_codigo                           = $P{pn_pel_codigo}
AND    paf.paf_pagado                           like $P{pv_paf_pagado}
ORDER BY  sed.est_codigo,campus,posgrado,cohorte,apellidos,nombres]]>
	</queryString>
