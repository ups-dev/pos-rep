	<parameter name="pv_usuario" class="java.lang.String"/>
	<parameter name="pn_alu_codigo" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="pn_est_codigo" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="pn_imprimeTitulo" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select ma.campus,ma.programa,ma.cohorte,
       ma.mal_codigo,ma.mat_codigo,
       ma.modulo,
       ma.mal_nivel,
       ma.mal_nro_horas,
       ma.semestre,
       ma.modalidad,
       ma.titulo,
       ma.total_horas,
       ma.cllc_cdg,
       ma.cedula,
       ma.estudiante,
       ma.fecha_inicio_carrera,
       ma.fecha_actual,
       ma.tiene_mencion_est,
       ma.tiene_mencion_proyecto,
       ma.total_menciones,
       ma.cdd_codigo_mencion,
       ma.materia_mencion,
       ma.tiene_mencion_proyecto,
       ca.fecha_semestre,
       ca.nota_final,
       ca.numero_veces,
       ca.tipo_aprobacion,
       ca.observaciones_matricula,
       NVL(ca.estado,ma.estado) estado,
       ca.para_contar,
       ca.para_promediar
from
       (select distinct campus, programa,est_codigo,cllc_cdg,cedula,estudiante,
                cohorte,total_horas,tiene_mencion_pro,
                cdd_codigo_mencion,
                materia_mencion,
           decode(tiene_mencion_pro,'N',p.mal_codigo,'S',decode(total_menciones,1,p.mal_codigo,decode(tiene_mencion_est,'S',decode(p.cdd_codigo_mencion,null,p.mal_codigo,(select mm.mal_codigo
                                                                                                                                                                                from ped.ped_malla_mencion mm
                                                                                                                                                                                where mm.mal_codigo = p.mal_codigo
                                                                                                                                                                                and   mm.cdd_codigo = p.cod_mencion_estudiante
                                                                                                                                                                                and   mm.aud_eliminado = 'N'),p.mal_codigo),p.mal_codigo))) mal_codigo,
           decode(tiene_mencion_pro,'N',p.mat_codigo,'S',decode(total_menciones,1,p.mat_codigo,decode(tiene_mencion_est,'S',decode(p.cdd_codigo_mencion,null,p.mat_codigo,(select mt.mat_codigo
                                                                                                                                                                    from ped.ped_malla_mencion mm,
                                                                                                                                                                         ped.ped_malla         ma,
                                                                                                                                                                         ped.ped_materia       mt
                                                                                                                                                                    where mm.mal_codigo = p.mal_codigo
                                                                                                                                                                    and   mm.cdd_codigo = p.cod_mencion_estudiante
                                                                                                                                                                    and   ma.mal_codigo = mm.mal_codigo
                                                                                                                                                                    and   mt.mat_codigo = ma.mat_codigo
                                                                                                                                                                    and   mm.aud_eliminado = 'N')),p.mat_codigo))) mat_codigo,
          decode(tiene_mencion_pro,'N',p.modulo,'S',decode(total_menciones,1,p.modulo,decode(tiene_mencion_est,'S',decode(p.cdd_codigo_mencion,null,p.modulo,(select mt.mat_descripcion
                                                                                                                                                                    from ped.ped_malla_mencion mm,
                                                                                                                                                                         ped.ped_malla         ma,
                                                                                                                                                                         ped.ped_materia       mt
                                                                                                                                                                    where mm.mal_codigo = p.mal_codigo
                                                                                                                                                                    and   mm.cdd_codigo = p.cod_mencion_estudiante
                                                                                                                                                                    and   ma.mal_codigo = mm.mal_codigo
                                                                                                                                                                    and   mt.mat_codigo = ma.mat_codigo
                                                                                                                                                                    and   mm.aud_eliminado = 'N')),p.modulo))) modulo,
           mal_nivel,
           mal_nro_horas,
           semestre,
           modalidad,
           tiene_mencion_pro tiene_mencion_proyecto,
           total_menciones,
           NVL(tiene_mencion_est,'N') tiene_mencion_est,
           cod_mencion_estudiante,
           mencion_estudiante,
           total_modulo_mencion,
           total_modulo,
           fecha_inicio_carrera,
           fecha_actual,
           decode(tiene_mencion_pro,'N',p.titulo,'S',decode(total_menciones,1,p.mencion_proyecto,decode(tiene_mencion_est,'S',titulo||', '||p.mencion_estudiante,null))) titulo,
           estado
           from(
               select distinct dcam.dee_descripcion              campus,
                 dpos.dee_descripcion                            programa,
                 pos.est_codigo,
                 pel.pel_descripcion ||' ('||extract (year from pel.pel_fecha_inicio)||' - '||extract (year from pel.pel_fecha_fin)||') ' cohorte,
                 mal.mal_codigo,
                 mal.mal_nivel,
                 mal.mal_nro_horas,
                 TO_CHAR(mal.mal_nivel, 'FMRN')||' '||'SEMESTRE' semestre,
                 mtr.mat_codigo,
                 mtr.mat_descripcion              modulo,
                 mod.mod_descripcion                             modalidad,
                 (select ppe.ppe_descripcion_texto
                  from ped.ped_par_pro_edu ppe,
                       ped.ped_tip_par_tip_pro_edu tpt
                  where ppe.tpt_codigo=tpt.tpt_codigo
                  and ppe.pre_numero = pre.pre_numero
                  and tpt.tpp_codigo = 114)                      tiene_mencion_pro,

                 (select count(cdd1.cdd_mencion)
                 from ped.ped_cde_dep  cdd1
                 where cdd1.dep_codigo = dep.dep_codigo
                 )                                              total_menciones,
                cac2.cac_descripcion                            titulo,
                cac2.cac_descripcion||', '||'MENCIÓN '||cdd.cdd_mencion  mencion_proyecto,
                (select 'S'
                  from    ped.ped_cde_dep cdd,
                          ins.ins_ipa_cdd iic,
                          ins.ins_ins_pro_aca ipa
                   where  cdd.cdd_codigo = iic.cdd_codigo
                   and    ipa.ipa_codigo = iic.ipa_codigo
                   and    ipa.ins_codigo = mat.ins_codigo
                   and    cdd.dep_codigo = dep.dep_codigo )  tiene_mencion_est,
                  (select cdd.cdd_codigo
                  from    ped.ped_cde_dep cdd,
                          ins.ins_ipa_cdd iic,
                          ins.ins_ins_pro_aca ipa
                   where  cdd.cdd_codigo = iic.cdd_codigo
                   and    ipa.ipa_codigo = iic.ipa_codigo
                   and    ipa.ins_codigo = mat.ins_codigo
                   and    cdd.dep_codigo = dep.dep_codigo)     cod_mencion_estudiante,
                  (select 'MENCIÓN '||cdd.cdd_mencion
                  from    ped.ped_cde_dep cdd,
                          ins.ins_ipa_cdd iic,
                          ins.ins_ins_pro_aca ipa
                   where  cdd.cdd_codigo = iic.cdd_codigo
                   and    ipa.ipa_codigo = iic.ipa_codigo
                   and    ipa.ins_codigo = mat.ins_codigo
                   and    cdd.dep_codigo = dep.dep_codigo)      mencion_estudiante,

                 (select ppe.ppe_descripcion_numero
                  from ped.ped_par_pro_edu ppe,
                       ped.ped_tip_par_tip_pro_edu tpt
                  where ppe.tpt_codigo=tpt.tpt_codigo
                  and ppe.pre_numero = pre.pre_numero
                  and tpt.tpp_codigo = 117)                     total_horas,
                 mat.cllc_cdg,
                 cli.cllc_ruc                                   cedula,
                 cli.cllc_nmb                                   estudiante,
                 inc.ins_fecha_inicio_carrera                   fecha_inicio_carrera,
                 'Cuenca, '||TO_CHAR(sysdate, 'DD')|| ' de '||DECODE(TO_CHAR(sysdate, 'MM'), '01', 'Enero', '02', 'Febrero', '03', 'Marzo', '04', 'Abril', '05', 'Mayo', '06', 'Junio',
                                       '07', 'Julio', '08', 'Agosto', '09', 'Septiembre', '10', 'Octubre', '11', 'Noviembre',
                                       '12', 'Diciembre')||' de '||
                 TO_CHAR(sysdate, 'YYYY')||'.'                  fecha_actual,
                 mam.CDD_codigo                                 cdd_codigo_mencion,
                 mam.mal_codigo                                 mal_codigo_mencion,
                 (select 'MENCIÓN '||cd.cdd_mencion
                 from ped.ped_cde_dep cd
                 where cd.cdd_codigo = mam.CDD_codigo)          materia_mencion,
                 decode(mam.cdd_codigo,null,0,1 )               total_modulo_mencion,
                 decode(mam.cdd_codigo,null,1,0 )               total_modulo,
                 'NO CURSADO' estado
          from   ped.ped_proyecto_educacion  pre,
                 ped.ped_malla               mal,
                 ped.ped_materia             mtr,
                 ped.ped_des_est_pro_edu     dep,
                 ped.ped_mod_pro_edu         mpe,
                 ped.ped_modalidad           mod,
                 org.org_descripcion_estructura dpos,
                 org.org_descripcion_estructura dcam,
                 org.org_estructura          pos,
                 org.org_estructura          cam,
                 ped.ped_cde_dep             cdd,
                 ped.ped_cam_des_est         cde,
                 ped.ped_cam_con_tip_tit     ctt,
                 ped.ped_campo_conocimiento  cac,
                 ped.ped_campo_conocimiento  cac2,
                 ped.ped_tipo_titulacion     tit,
                 mat.mat_matricula           mat,
                 sigac.cliente_local         cli,
                 ins.ins_inscripcion         inc,
                 org.org_periodo_estructura  pee,
                 org.org_periodo_lectivo     pel,
                 ped.ped_malla_mencion       mam
          where   pre.pre_numero  = mal.pre_numero
          AND     mtr.mat_codigo  = mal.mat_codigo
          AND     dep.pre_numero  = pre.pre_numero
          AND     pre.pre_numero  = mpe.pre_numero
          AND     mod.mod_codigo  = mpe.mod_codigo
          AND     dep.dee_codigo  = dpos.dee_codigo
          AND     pos.dee_codigo  = dpos.dee_codigo
          AND     cam.est_codigo  = pos.est_codigo_padre
          AND     cam.dee_codigo  = dcam.dee_codigo
          AND     mal.aud_eliminado = 'N'
          AND     pos.est_codigo  = $P{pn_est_codigo}
          AND     dep.dep_codigo  = cdd.dep_codigo
          AND     cde.cde_codigo  = cdd.cde_codigo
          AND     ctt.cct_codigo  = cde.cct_codigo
          AND     cac.cac_codigo  = ctt.cac_codigo
          AND     cac.cac_codigo_padre = cac2.cac_codigo
          AND     tit.tit_codigo       = ctt.tit_codigo
          AND     tit.ned_codigo       = 2
          AND     mat.dep_codigo  = dep.dep_codigo
          AND     inc.alu_codigo  = $P{pn_alu_codigo}
          AND     mat.cllc_cdg    = cli.cllc_cdg
          AND     mat.ins_codigo  = inc.ins_codigo
          AND     mat.pee_codigo  = pee.pee_codigo
          AND     pee.pel_codigo  = pel.pel_codigo
          AND     mal.mal_codigo  = mam.mal_codigo(+)
          )p)ma,
          (SELECT  dsed.dee_descripcion        sede,
                    dcam.dee_descripcion        campus,
                    pos.est_codigo,
                    dpos.dee_descripcion        posgrado,
                    mod.mod_descripcion         modalidad,
                    pel.pel_descripcion, pel.pel_codigo,
                    extract (year from pee.pee_fecha_inicial)||' - '||extract (year from pee.pee_fecha_final) fecha_semestre,
                    cli.cllc_cdg,
                    mtr.mat_codigo,
                    mtr.mat_descripcion         ,
                    mal.mal_codigo,
                    mal.mal_nivel               nivel,
                    mal.mal_nro_horas,
                    cal.cal_nota_final          nota_final,
                    cal.cal_numero_veces        numero_veces,
                    cal.tap_codigo,
                    tap.tap_descripcion         tipo_aprobacion,
                    mat.mat_observaciones       observaciones_matricula,
                    decode(cal.cal_aprobado,'S','APROBADO',DECODE(pee.pee_vigente,'S','CURSANDO',DECODE(cal.cal_aprobado,'S','APROBADO','REPROBADO')) ) estado,
                    decode(cal.cal_aprobado,'S',1,DECODE(pee.pee_vigente,'S',0,DECODE(cal.cal_aprobado,'S',1,0)) ) para_contar,
                    DECODE(cal.cal_aprobado,'S',cal.cal_nota_final) para_promediar
          FROM    mat.mat_matricula           mat,
                  cal.cal_calificacion        cal,
                  cal.cal_tipo_aprobacion     tap,
                  fac.fac_pago_factura        paf,
                  sigac.cliente_local         cli,
                  ped.ped_malla               mal,
                  ped.ped_materia             mtr,
                  ped.ped_proyecto_educacion  pre,
                  ped.ped_des_est_pro_edu     dep,
                  ped.ped_mod_pro_edu         mpe,
                  ped.ped_modalidad           mod,
                  org.org_descripcion_estructura dpos,
                  org.org_descripcion_estructura dcam,
                  org.org_descripcion_estructura dsed,
                  org.org_estructura          pos,
                  org.org_estructura          cam,
                  org.org_estructura          sed,
                  org.org_periodo_estructura  pee,
                  org.org_periodo_lectivo     pel,
                  ins.ins_alumno              alu
          WHERE   mat.mat_codigo  = cal.mat_codigo
          AND     mat.mat_codigo  = paf.ins_codigo
          AND     mat.cllc_cdg    = cli.cllc_cdg
          AND     tap.tap_codigo  = cal.tap_codigo
          AND     cal.mal_codigo  = mal.mal_codigo
          AND     mal.mat_codigo  = mtr.mat_codigo
          AND     mal.pre_numero  = pre.pre_numero
          AND     mat.pee_codigo  = pee.pee_codigo
          AND     dep.pre_numero  = pre.pre_numero
          AND     pre.pre_numero  = mpe.pre_numero
          AND     mod.mod_codigo  = mpe.mod_codigo
          AND     dep.dee_codigo  = dpos.dee_codigo
          AND     pee.est_codigo  = pos.est_codigo
          AND     pos.dee_codigo  = dpos.dee_codigo
          AND     cam.est_codigo  = pos.est_codigo_padre
          AND     cam.dee_codigo  = dcam.dee_codigo
          AND     sed.est_codigo  = cam.est_codigo_padre
          AND     sed.dee_codigo  = dsed.dee_codigo
          AND     alu.cllc_cdg    = mat.cllc_cdg
          AND     mat.mat_tipo_proceso = 'M'
          AND     mat.aud_eliminado = 'N'
          AND     mat.mat_anulado   = 'N'
          AND     paf.tip_codigo  = 4
          AND     pel.pel_codigo  = pee.pel_codigo
          AND     pos.est_codigo  = $P{pn_est_codigo}
          AND     cal.aud_eliminado = 'N'
          AND     cal.cal_retirado  = 'N'
          AND     paf.paf_pagado    = 'S'
          AND     alu.alu_codigo    = $P{pn_alu_codigo}
    )ca
      where    ma.est_codigo  = ca.est_codigo (+)
      AND      ma.mal_codigo = ca.mal_codigo(+)
      AND      ma.cllc_cdg   = ca.cllc_cdg(+)
      AND      ma.mat_codigo is not null
ORDER BY semestre,materia_mencion nulls first,modulo]]>
	</queryString>
