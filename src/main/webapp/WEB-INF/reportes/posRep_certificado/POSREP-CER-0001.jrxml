	<queryString>
		<![CDATA[select distinct sede, campus,est_codigo,posgrado,pre_numero,
       modalidad,
       pel_codigo,
       pel_descripcion,
       cohorte,
       fecha_semestre,
       cllc_cdg,
       cedula,
       estudiante,
       mat_codigo,
       modulo,
       nivel,
       mal_nro_horas,
       nota_final,
       numero_veces,estado,
       fecha_actual,
       observaciones,
       total_horas,
       tap_codigo,
       tipo_aprobacion,
       tiene_mencion,
       NVL(tiene_mencion_est,'N') tiene_mencion_est,
       total_menciones,
       decode( tiene_mencion,'N',decode(titulo,'OTRO',cde_des_tit_opcional,titulo),'S',(decode(total_menciones,1,mencion_proyecto,decode(mencion_estudiante,null,null,mencion_estudiante)))) titulo

from

(select distinct dse.dee_descripcion        sede,
        dcm.dee_descripcion        campus,
        pos.est_codigo,
        dpos.dee_descripcion        posgrado,
        mod.mod_descripcion         modalidad,
        pel.pel_descripcion,        pel.pel_codigo,
        pel.pel_descripcion ||' ('||extract (year from pel.pel_fecha_inicio)||' - '||extract (year from pel.pel_fecha_fin)||') ' cohorte,
        TRIM(TO_CHAR(pee.pee_fecha_inicial,'MONTH'))||' '||extract (year from pee.pee_fecha_inicial)||' - '||TRIM(TO_CHAR(pee.pee_fecha_final,'MONTH'))||' '||extract (year from pee.pee_fecha_final) fecha_semestre,
        cli.cllc_cdg,
        cli.cllc_ruc                cedula,
        cli.cllc_nmb                estudiante,
        mtr.mat_codigo,
        mtr.mat_descripcion         modulo,
        TO_CHAR(mal.mal_nivel, 'FMRN')||' SEMESTRE' nivel,
        mal.mal_nro_horas,
        cal.cal_nota_final          nota_final,
        TO_CHAR(cal.cal_numero_veces, 'FMRN')numero_veces,
        cal.tap_codigo,
        decode(tap.tap_codigo,1,'N',4,'H.A.C',5,'H.V.C',NULL) tipo_aprobacion,
        mta.mat_observaciones       observaciones_matricula,
        decode(cal.cal_aprobado,'S','APROBADO',DECODE(pee.pee_vigente,'S','CURSANDO',DECODE(cal.cal_aprobado,'S','APROBADO','REPROBADO')) ) estado,
        decode(dse.dee_descripcion,'MATRIZ CUENCA','Cuenca',INITCAP(dse.dee_descripcion))||', '||TO_CHAR(sysdate, 'DD')|| ' de '||DECODE(TO_CHAR(sysdate, 'MM'), '01', 'Enero', '02', 'Febrero', '03', 'Marzo', '04', 'Abril', '05', 'Mayo', '06', 'Junio',
                                       '07', 'Julio', '08', 'Agosto', '09', 'Septiembre', '10', 'Octubre', '11', 'Noviembre',
                                       '12', 'Diciembre')||' de '||
               TO_CHAR(sysdate, 'YYYY')||'.' fecha_actual,

        cal.caa_observaciones_leg observaciones,
        ( select ppe.PPE_DESCRIPCION_NUMERO
          from PED.PED_PAR_PRO_EDU ppe,
               PED.PED_TIP_PAR_TIP_PRO_EDU tpt
          where ppe.TPT_CODIGO=tpt.TPT_CODIGO
          and ppe.PRE_NUMERO = pre.pre_numero
          and tpt.TPP_CODIGO = 117) total_horas,
          ( select ppe.PPE_DESCRIPCION_TEXTO
          from PED.PED_PAR_PRO_EDU ppe,
               PED.PED_TIP_PAR_TIP_PRO_EDU tpt
          where ppe.TPT_CODIGO=tpt.TPT_CODIGO
          and ppe.PRE_NUMERO = pre.pre_numero
          and tpt.TPP_CODIGO = 114) tiene_mencion,
         (select 'S'
          from    ped.ped_cde_dep cdd,
                  ins.ins_ipa_cdd iic,
                  ins.ins_ins_pro_aca ipa
           where  cdd.cdd_codigo = iic.cdd_codigo
           and    ipa.ipa_codigo = iic.ipa_codigo
           and    ipa.ins_codigo = inc.ins_codigo
           and    cdd.dep_codigo = dep.dep_codigo
                  ) tiene_mencion_est,
          (select cac2.cac_descripcion||', '||'MENCIÓN '||cdd.cdd_mencion
          from    ped.ped_cde_dep cdd,
                  ins.ins_ipa_cdd iic,
                  ins.ins_ins_pro_aca ipa
           where  cdd.cdd_codigo = iic.cdd_codigo
           and    ipa.ipa_codigo = iic.ipa_codigo
           and    ipa.ins_codigo = inc.ins_codigo
           and    cdd.dep_codigo = dep.dep_codigo
                  ) mencion_estudiante,
           (select count(cdd1.cdd_mencion)
           from ped.ped_cde_dep  cdd1
           where cdd1.dep_codigo = dep.dep_codigo
           )total_menciones,
           cac2.cac_descripcion titulo,
           cac2.cac_descripcion||', '||'MENCIÓN '||cdd.cdd_mencion  mencion_proyecto,
   cde.cde_des_tit_opcional,
   pre.pre_numero

from mat.mat_matricula    mta,
     fac.fac_pago_factura paf,
     ins.ins_inscripcion inc,
     ins.ins_oferta_inscripcion_inicial oii,
     sigac.cliente_local         cli,
     cal.cal_calificacion        cal,
     cal.cal_tipo_aprobacion     tap,
     ped.ped_proyecto_educacion  pre,
     ped.ped_malla               mal,
     ped.ped_materia             mtr,
     ped.ped_des_est_pro_edu     dep,
     ped.ped_mod_pro_edu         mpe,
     ped.ped_est_mod_pro_edu     emp,
     ped.ped_modalidad           mod,
     org.org_descripcion_estructura dpos,
     org.org_estructura             pos,
     org.org_descripcion_estructura dcm,
     org.org_estructura             cam,
     org.org_descripcion_estructura dse,
     org.org_estructura             sed,
     org.org_periodo_estructura     pee,
     org.org_periodo_lectivo        pel,
     ped.ped_cam_des_est            cde,
     ped.ped_cde_dep                cdd,
     ped.ped_cam_con_tip_tit        ctt,
     ped.ped_campo_conocimiento     cac,
     ped.ped_campo_conocimiento     cac2,
     ped.ped_tipo_titulacion        tit

where mta.mat_codigo = paf.ins_codigo
and   mta.ins_codigo = inc.ins_codigo
and   oii.oii_codigo = inc.oii_codigo
and   mta.cllc_cdg   = cli.cllc_cdg
and   mta.mat_tipo_proceso = 'M'
and   mta.mat_anulado      = 'N'
and   mta.aud_eliminado    = 'N'
and   paf.tip_codigo  = 4
and   paf.paf_pagado  = 'S'
and   mta.mat_codigo  = cal.mat_codigo
and   tap.tap_codigo  = cal.tap_codigo
and   cal.aud_eliminado = 'N'
and   cal.cal_retirado  = 'N'
and   cal.mal_codigo    = mal.mal_codigo
and   pre.pre_numero    = mal.pre_numero
and   mtr.mat_codigo    = mal.mat_codigo
and   pre.pre_numero    = dep.pre_numero
and   pre.pre_numero    = mpe.pre_numero
and   mod.mod_codigo    = mpe.mod_codigo
and   mpe.mpe_codigo    = emp.mpe_codigo
and   pos.est_codigo    = oii.est_codigo
and   pos.dee_codigo    = dpos.dee_codigo
and   pos.est_codigo_padre = cam.est_codigo
and   cam.dee_codigo       = dcm.dee_codigo
and   cam.est_codigo_padre = sed.est_codigo
and   sed.dee_codigo       = dse.dee_codigo
and   pos.est_codigo_padre = emp.est_codigo
and   mta.pee_codigo       = pee.pee_codigo
and   pee.pel_codigo       = pel.pel_codigo

and   dep.dep_codigo  = cdd.dep_codigo
and   cde.cde_codigo  = cdd.cde_codigo
and   ctt.cct_codigo  = cde.cct_codigo
and   cac.cac_codigo  = ctt.cac_codigo
and   cac.cac_codigo_padre = cac2.cac_codigo
and   tit.tit_codigo       = ctt.tit_codigo
and   tit.ned_codigo       = 2
and   pee.pel_codigo  = $P{pn_pel_codigo}
and   pos.est_codigo  IN (SELECT est_codigo e
                            FROM   org.org_estructura e
                            WHERE  e.est_nivel  = 4
                            START WITH e.est_codigo = $P{pn_est_codigo}
                            CONNECT BY PRIOR e.est_codigo = e.est_codigo_padre )
and   pee.pee_nivel  = $P{pv_nivel}
and  inc.alu_codigo  = $P{pn_alu_codigo}
) p
ORDER   BY campus,posgrado,nivel,modulo,estudiante]]>
