	<queryString>
		<![CDATA[SELECT  dsed.dee_descripcion        sede,
        dcam.dee_descripcion        campus,
        dpos.dee_descripcion        posgrado,
        mod.mod_descripcion         modalidad,
        pel.pel_descripcion, pel.pel_codigo,
        TRIM(TO_CHAR(pee.pee_fecha_inicial,'Month'))||' '||extract (year from pee.pee_fecha_inicial)||' - '||TRIM(TO_CHAR(pee.pee_fecha_final,'Month'))||' '||extract (year from pee.pee_fecha_final) fecha_semestre,
        cli.cllc_cdg,
        cli.cllc_ruc                cedula,
        cli.cllc_nmb                estudiante,
        mtr.mat_codigo,
        mtr.mat_descripcion         modulo,
        mal.mal_nivel               nivel,
        mal.mal_nro_horas,
        cal.cal_nota_final          nota_final,
        TO_CHAR(cal.cal_numero_veces, 'FMRN')numero_veces,
        cal.tap_codigo,
        --tap.tap_descripcion         tipo_aprobacion,
        decode(tap.tap_codigo,1,'N',5,'H.A.C',6,'H.V.C',NULL) tipo_aprobacion,
        mat.mat_observaciones       observaciones_matricula,
decode(cal.cal_aprobado,'S','APROBADO',DECODE(pee.pee_vigente,'S','CURSANDO',DECODE(cal.cal_aprobado,'S','APROBADO','REPROBADO')) ) estado,
decode(dsed.dee_descripcion,'MATRIZ CUENCA','Cuenca',INITCAP(dsed.dee_descripcion))||', '||TO_CHAR(sysdate, 'DD')|| ' de '||DECODE(TO_CHAR(sysdate, 'MM'), '01', 'Enero', '02', 'Febrero', '03', 'Marzo', '04', 'Abril', '05', 'Mayo', '06', 'Junio',
                                       '07', 'Julio', '08', 'Agosto', '09', 'Septiembre', '10', 'Octubre', '11', 'Noviembre',
                                       '12', 'Diciembre')||' de '||
                 TO_CHAR(sysdate, 'YYYY')||'.' fecha_actual,
        cal.caa_observaciones_leg observaciones

FROM    mat.mat_matricula           mat,
        cal.cal_calificacion        cal,
        cal.cal_tipo_aprobacion     tap,
        fac.fac_pago_factura        paf,
        sigac.cliente_local         cli,
        ped.ped_malla               mal,
        ped.ped_materia             mtr,
        ped.ped_proyecto_educacion  pre,
        ped.ped_des_est_pro_edu     dep,
        ped.ped_mod_pro_edu         mpe,
        ped.ped_modalidad           mod,
        org.org_descripcion_estructura dpos,
        org.org_descripcion_estructura dcam,
        org.org_descripcion_estructura dsed,
        org.org_estructura          pos,
        org.org_estructura          cam,
        org.org_estructura          sed,
        org.org_periodo_estructura  pee,
        org.org_periodo_lectivo     pel,
        ins.ins_alumno              alu
WHERE   mat.mat_codigo  = cal.mat_codigo
AND     mat.mat_codigo  = paf.ins_codigo
AND     mat.cllc_cdg    = cli.cllc_cdg
AND     tap.tap_codigo  = cal.tap_codigo
AND     cal.mal_codigo  = mal.mal_codigo
AND     mal.mat_codigo  = mtr.mat_codigo
AND     mal.pre_numero  = pre.pre_numero
AND     mat.pee_codigo  = pee.pee_codigo
AND     dep.pre_numero  = pre.pre_numero
AND     pre.pre_numero  = mpe.pre_numero
AND     mod.mod_codigo  = mpe.mod_codigo
AND     dep.dee_codigo  = dpos.dee_codigo
AND     pee.est_codigo  = pos.est_codigo
AND     pos.dee_codigo  = dpos.dee_codigo
AND     cam.est_codigo  = pos.est_codigo_padre
AND     cam.dee_codigo  = dcam.dee_codigo
AND     sed.est_codigo  = cam.est_codigo_padre
AND     sed.dee_codigo  = dsed.dee_codigo
AND     alu.cllc_cdg    = mat.cllc_cdg
AND     mat.mat_tipo_proceso = 'M'
AND     pre.tpe_codigo  = 8
AND     paf.tip_codigo  = 4
AND     pel.pel_codigo  = pee.pel_codigo
AND     pee.pel_codigo  = $P{pn_pel_codigo}
AND     pos.est_codigo  IN (SELECT est_codigo e
                            FROM   org.org_estructura e
                            WHERE  e.est_nivel  = 4
                            START WITH e.est_codigo = $P{pn_est_codigo}
                            CONNECT BY PRIOR e.est_codigo = e.est_codigo_padre)
AND     mal.mal_nivel   = $P{pv_nivel}
AND     cal.aud_eliminado = 'N'
AND     cal.cal_retirado  = 'N'
AND     mat.aud_eliminado = 'N'
AND     paf.paf_pagado    = 'S'
AND     alu.alu_codigo    = $P{pn_alu_codigo}
ORDER   BY campus,posgrado,nivel,modulo,estudiante]]>
	</queryString>
