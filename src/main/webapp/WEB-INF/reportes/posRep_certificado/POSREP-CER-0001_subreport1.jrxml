	<queryString>
		<![CDATA[SELECT cam.per_codigo,
       ned.ned_abreviatura||' '||per.per_nombres||' '||per.per_apellidos secre_campus,
       'SECRETARIA CAMPUS'||' '||dca.dee_descripcion campus,
       'SEDE'||' '||dse.dee_descripcion sede,
       'Que, la firma y rúbrica estampada en el presente documento corresponden a '||INITCAP(ned.ned_abreviatura)||' '||INITCAP(per.per_nombres||' '||per.per_apellidos)||
       ' Secretaria del Campus '||INITCAP(dca.dee_descripcion)||' de la Universidad Politécnica Salesiana, Sede '||INITCAP(dse.dee_descripcion)||'.' leyenda,
       initcap (seg.seg_lugar_certifica)||', '||TO_CHAR(sysdate, 'DD')|| ' de '||
        DECODE(TO_CHAR(sysdate, 'MM'), '01', 'Enero', '02', 'Febrero', '03', 'Marzo', '04', 'Abril', '05', 'Mayo', '06', 'Junio',
                                       '07', 'Julio', '08', 'Agosto', '09', 'Septiembre', '10', 'Octubre', '11', 'Noviembre',
                                       '12', 'Diciembre')||' de '||
       TO_CHAR(sysdate, 'YYYY')||'.' fecha,INITCAP(seg.seg_nombre) secre_general,seg.seg_titulo

FROM org.org_estructura cam,
     gth.gth_persona per,
     gth.gth_educacion_formal        edf,
     gth.gth_nivel_educacion_detalle ned,
     org.org_descripcion_estructura  dca,
     org.org_estructura              sed,
     org.org_descripcion_estructura  dse,
     rep.rep_secretario_general      seg
WHERE  edf.edf_principal = 'S'
AND    edf.per_codigo    = per.per_codigo
AND    ned.ned_codigo    = edf.ned_codigo
AND    ned.nie_codigo    = edf.nie_codigo
AND    edf.edf_eliminado = 'N'
AND    ned.ned_eliminado = 'N'
AND    per.per_eliminado = 'N'
AND    per.per_codigo    = cam.per_codigo
AND    cam.est_codigo    = $P{pn_campus}
AND    cam.dee_codigo    = dca.dee_codigo
AND    cam.est_codigo_padre = sed.est_codigo
AND    sed.dee_codigo     = dse.dee_codigo
AND    seg.seg_codigo     = $P{pn_seg_codigo}
AND    rownum             = 1]]>
	</queryString>
