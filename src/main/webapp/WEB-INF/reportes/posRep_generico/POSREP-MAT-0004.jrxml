	<queryString>
		<![CDATA[--MATRICULADOS CON FECHA DE COHORTE VIGENTE
SELECT DISTINCT dsed.dee_descripcion        sede,
        dcam.dee_descripcion        campus,
        dpos.dee_descripcion        posgrado,
        mod.mod_descripcion         modalidad,
        pel.pel_codigo,
        pel.pel_descripcion         cohorte,
        cli.cllc_ruc                cedula,
        alu.alu_apellidos||' '||alu.alu_nombres   estudiante,
        alu.alu_fecha_nacimiento    fecha_nacimiento,
       (select ag1.arg_nacionalidad
        from org.org_area_geografica ag5,
              org.org_area_geografica ag4,
              org.org_area_geografica ag3,
              org.org_area_geografica ag2,
              org.org_area_geografica ag1
        where ag5.arg_codigo_padre = ag4.arg_codigo
        and   ag4.arg_codigo_padre = ag3.arg_codigo
        and   ag3.arg_codigo_padre = ag2.arg_codigo
        and   ag2.arg_codigo_padre = ag1.arg_codigo
        and   ag5.arg_codigo       = alu.arg_codigo_nac)   nacionalidad,
       cli.cllc_calle               direccion,
       cli.cllc_fono                telefono,
       cli.cllc_celular             celular,
       alu.alu_email                correo_institucional,
       cli.cllc_email               correo_personal,inc.ins_codigo
FROM    mat.mat_matricula           mat,
        cal.cal_calificacion        cal,
        fac.fac_pago_factura        paf,
        sigac.cliente_local         cli,
        ofe.ofe_grupo               gru,
        ped.ped_malla               mal,
        ped.ped_proyecto_educacion  pre,
        ped.ped_des_est_pro_edu     dep,
        org.org_descripcion_estructura dpos,
        org.org_descripcion_estructura dcam,
        org.org_descripcion_estructura dsed,
        org.org_estructura          pos,
        org.org_estructura          cam,
        org.org_estructura          sed,
        org.org_periodo_estructura  pee,
        org.org_periodo_lectivo     pel,
        ins.ins_alumno              alu,
        ins.ins_inscripcion         inc,
        ins.ins_oferta_inscripcion_inicial       oii,
        ped.ped_modalidad           mod
WHERE   mat.mat_codigo  = cal.mat_codigo
AND     mat.mat_codigo  = paf.ins_codigo
AND     mat.cllc_cdg    = cli.cllc_cdg
AND     cal.gru_codigo  = gru.gru_codigo(+)
AND     cal.mal_codigo  = mal.mal_codigo
AND     mal.pre_numero  = pre.pre_numero
AND     mat.pee_codigo  = pee.pee_codigo
AND     dep.dep_codigo  = mat.dep_codigo --
AND     dep.pre_numero  = pre.pre_numero
AND     dep.dee_codigo  = dpos.dee_codigo
AND     pee.est_codigo  = pos.est_codigo
AND     pos.dee_codigo  = dpos.dee_codigo
AND     cam.est_codigo  = pos.est_codigo_padre
AND     cam.dee_codigo  = dcam.dee_codigo
AND     sed.est_codigo  = cam.est_codigo_padre
AND     sed.dee_codigo  = dsed.dee_codigo
AND     alu.cllc_cdg    = mat.cllc_cdg
AND     mat.mat_tipo_proceso = 'M'
AND     paf.tip_codigo  = 4
AND     paf.paf_pagado  = 'S'
AND     pel.pel_codigo  = pee.pel_codigo
AND     pee.pel_codigo  like $P{pv_pel_codigo}
AND     pos.est_codigo  IN (SELECT est_codigo e
                            FROM   org.org_estructura e
                            WHERE  e.est_nivel  = 4
                            START WITH e.est_codigo = $P{pn_est_codigo}
                            CONNECT BY PRIOR e.est_codigo = e.est_codigo_padre)
AND     cal.aud_eliminado = 'N'
AND     cal.cal_retirado  = 'N'
AND     mat.aud_eliminado = 'N'
AND     mat.mat_anulado   = 'N'
AND     inc.ins_codigo    = mat.ins_codigo
AND     oii.oii_codigo    = inc.oii_codigo
AND     mod.mod_codigo    = oii.mod_codigo
AND     oii.mod_codigo    like $P{pv_mod_codigo}
AND     inc.ins_codigo  NOT  IN  (select rei.ins_codigo  --SIN DECLARATORIA DE APTITUD
                                  from sse.sse_tipo_resolucion tir,
                                       sse.sse_tip_con_tip_res tct,
                                       sse.sse_antecedente     ant,
                                       sse.sse_resolucion      res,
                                       sse.sse_resolucion_inscripcion rei
                                  where tir.tir_codigo = tct.tir_codigo
                                  and   tct.tct_codigo = ant.tct_codigo
                                  and   ant.ant_codigo = res.ant_codigo
                                  and   tir.tir_codigo  = 9
                                  and   res.aud_eliminado = 'N'
                                  and   res.res_estado  = 'A'
                                  and   res.res_codigo   = rei.res_codigo)
AND  pee.pee_fecha_final_cohorte >= SYSDATE

UNION
--MATRICULADOS CON TRABAJO DE TITULACION O PRORROGAS 1,2
SELECT DISTINCT dsed.dee_descripcion        sede,
        dcam.dee_descripcion        campus,
        dpos.dee_descripcion        posgrado,
        mod.mod_descripcion         modalidad,
        pel.pel_codigo,
        pel.pel_descripcion         cohorte,
        cli.cllc_ruc                cedula,
        alu.alu_apellidos||' '||alu.alu_nombres   estudiante,
        alu.alu_fecha_nacimiento    fecha_nacimiento,
       (select ag1.arg_nacionalidad
        from org.org_area_geografica ag5,
              org.org_area_geografica ag4,
              org.org_area_geografica ag3,
              org.org_area_geografica ag2,
              org.org_area_geografica ag1
        where ag5.arg_codigo_padre = ag4.arg_codigo
        and   ag4.arg_codigo_padre = ag3.arg_codigo
        and   ag3.arg_codigo_padre = ag2.arg_codigo
        and   ag2.arg_codigo_padre = ag1.arg_codigo
        and   ag5.arg_codigo       = alu.arg_codigo_nac)   nacionalidad,
       cli.cllc_calle               direccion,
       cli.cllc_fono                telefono,
       cli.cllc_celular             celular,
       alu.alu_email                correo_institucional,
       cli.cllc_email               correo_personal,inc.ins_codigo
FROM    mat.mat_matricula           mat,
        cal.cal_calificacion        cal,
        fac.fac_pago_factura        paf,
        sigac.cliente_local         cli,
        ofe.ofe_grupo               gru,
        ped.ped_malla               mal,
        ped.ped_proyecto_educacion  pre,
        ped.ped_des_est_pro_edu     dep,
        org.org_descripcion_estructura dpos,
        org.org_descripcion_estructura dcam,
        org.org_descripcion_estructura dsed,
        org.org_estructura          pos,
        org.org_estructura          cam,
        org.org_estructura          sed,
        org.org_periodo_estructura  pee,
        org.org_periodo_lectivo     pel,
        ins.ins_alumno              alu,
        ins.ins_inscripcion         inc,
        ins.ins_oferta_inscripcion_inicial       oii,
        ped.ped_modalidad           mod
WHERE   mat.mat_codigo  = cal.mat_codigo
AND     mat.mat_codigo  = paf.ins_codigo
AND     mat.cllc_cdg    = cli.cllc_cdg
AND     cal.gru_codigo  = gru.gru_codigo(+)
AND     cal.mal_codigo  = mal.mal_codigo
AND     mal.pre_numero  = pre.pre_numero
AND     mat.pee_codigo  = pee.pee_codigo
AND     dep.dep_codigo  = mat.dep_codigo --
AND     dep.pre_numero  = pre.pre_numero
AND     dep.dee_codigo  = dpos.dee_codigo
AND     pee.est_codigo  = pos.est_codigo
AND     pos.dee_codigo  = dpos.dee_codigo
AND     cam.est_codigo  = pos.est_codigo_padre
AND     cam.dee_codigo  = dcam.dee_codigo
AND     sed.est_codigo  = cam.est_codigo_padre
AND     sed.dee_codigo  = dsed.dee_codigo
AND     alu.cllc_cdg    = mat.cllc_cdg
AND     mat.mat_tipo_proceso = 'M'
AND     paf.tip_codigo  = 4
AND     paf.paf_pagado  = 'S'
AND     pel.pel_codigo  = pee.pel_codigo
AND     pee.pel_codigo  like $P{pv_pel_codigo}
AND     pos.est_codigo  IN (SELECT est_codigo e
                            FROM   org.org_estructura e
                            WHERE  e.est_nivel  = 4
                            START WITH e.est_codigo = $P{pn_est_codigo}
                            CONNECT BY PRIOR e.est_codigo = e.est_codigo_padre)
AND     cal.aud_eliminado = 'N'
AND     cal.cal_retirado  = 'N'
AND     mat.aud_eliminado = 'N'
AND     mat.mat_anulado   = 'N'
AND     inc.ins_codigo    = mat.ins_codigo
AND     oii.oii_codigo    = inc.oii_codigo
AND     mod.mod_codigo    = oii.mod_codigo
AND     oii.mod_codigo    like $P{pv_mod_codigo}
AND     inc.ins_codigo  NOT  IN  (select distinct rei.ins_codigo --SIN DECLARATORIA DE APTITUD
                                  from sse.sse_tipo_resolucion tir,
                                       sse.sse_tip_con_tip_res tct,
                                       sse.sse_antecedente     ant,
                                       sse.sse_resolucion      res,
                                       sse.sse_resolucion_inscripcion rei
                                  where tir.tir_codigo = tct.tir_codigo
                                  and   tct.tct_codigo = ant.tct_codigo
                                  and   ant.ant_codigo = res.ant_codigo
                                  and   tir.tir_codigo  = 9
                                  and   res.aud_eliminado = 'N'
                                  and   res.res_estado  = 'A'
                                  and   res.res_codigo   = rei.res_codigo)
AND  inc.ins_codigo  IN    (select distinct ipa.ins_codigo -- CON TRABAJO DE TITULACION O PRORROGAS 1,2
                            from stg.stg_trabajo_graduacion trg,
                                 ins.ins_ins_pro_aca ipa,
                                 stg.stg_prorroga_trabajo prt,
                                 org.org_estructura       pos,
                                 org.org_descripcion_estructura dpos
                            where ipa.ipa_codigo = trg.ipa_codigo
                            and   trg.tdg_aprobado = 'S'
                            and   trg.tdg_codigo  = prt.tdg_codigo(+)
                            and   prt.prt_numero_prorroga(+)IN (1,2)
                            and   prt.prt_vigente(+) = 'S'
                            and   pos.est_codigo     = trg.est_codigo
                            and   pos.dee_codigo     = dpos.dee_codigo
                            and   dpos.tie_codigo    = 6
                            and   ((trg.tdg_fecha_vencimiento >= SYSDATE) OR (prt.prt_fecha_vencimiento >= SYSDATE))
                            )
UNION
--MATRICULADOS Y CON FECHA DE ÚLTIMO MÓDULO + 6 MESES
SELECT DISTINCT dsed.dee_descripcion sede,
        dcam.dee_descripcion        campus,
        dpos.dee_descripcion        posgrado,
        mod.mod_descripcion         modalidad,
        pel.pel_codigo,
        pel.pel_descripcion         cohorte,
        cli.cllc_ruc                cedula,
        alu.alu_apellidos||' '||alu.alu_nombres   estudiante,
        alu.alu_fecha_nacimiento    fecha_nacimiento,
       (select ag1.arg_nacionalidad
        from org.org_area_geografica ag5,
              org.org_area_geografica ag4,
              org.org_area_geografica ag3,
              org.org_area_geografica ag2,
              org.org_area_geografica ag1
        where ag5.arg_codigo_padre = ag4.arg_codigo
        and   ag4.arg_codigo_padre = ag3.arg_codigo
        and   ag3.arg_codigo_padre = ag2.arg_codigo
        and   ag2.arg_codigo_padre = ag1.arg_codigo
        and   ag5.arg_codigo       = alu.arg_codigo_nac)   nacionalidad,
       cli.cllc_calle               direccion,
       cli.cllc_fono                telefono,
       cli.cllc_celular             celular,
       alu.alu_email                correo_institucional,
       cli.cllc_email               correo_personal,inc.ins_codigo
FROM    mat.mat_matricula           mat,
        cal.cal_calificacion        cal,
        fac.fac_pago_factura        paf,
        ped.ped_malla               mal,
        ofe.ofe_grupo               gru,
        ofe.ofe_distributivo        dis,
        sigac.cliente_local         cli,
        ped.ped_proyecto_educacion  pre,
        ped.ped_des_est_pro_edu     dep,
        org.org_descripcion_estructura dpos,
        org.org_descripcion_estructura dcam,
        org.org_descripcion_estructura dsed,
        org.org_estructura          pos,
        org.org_estructura          cam,
        org.org_estructura          sed,
        org.org_periodo_estructura  pee,
        org.org_periodo_lectivo     pel,
        ins.ins_alumno              alu,
        ins.ins_inscripcion         inc,
        ins.ins_oferta_inscripcion_inicial oii,
        ped.ped_modalidad           mod
WHERE   mat.mat_codigo  = cal.mat_codigo
AND     mat.mat_codigo  = paf.ins_codigo
AND     cal.mal_codigo  = mal.mal_codigo
AND     cal.gru_codigo  = gru.gru_codigo
AND     dis.gru_codigo = gru.gru_codigo
AND     mat.mat_tipo_proceso = 'M'
AND     paf.tip_codigo  = 4
AND     paf.paf_pagado  = 'S'
AND     mat.cllc_cdg    = cli.cllc_cdg
AND     mal.pre_numero  = pre.pre_numero
AND     mat.pee_codigo  = pee.pee_codigo
AND     dep.dep_codigo  = mat.dep_codigo --
AND     dep.pre_numero  = pre.pre_numero
AND     dep.dee_codigo  = dpos.dee_codigo
AND     pee.est_codigo  = pos.est_codigo
AND     pos.dee_codigo  = dpos.dee_codigo
AND     cam.est_codigo  = pos.est_codigo_padre
AND     cam.dee_codigo  = dcam.dee_codigo
AND     sed.est_codigo  = cam.est_codigo_padre
AND     sed.dee_codigo  = dsed.dee_codigo
AND     alu.cllc_cdg    = mat.cllc_cdg
AND     pel.pel_codigo  = pee.pel_codigo
AND     pee.pel_codigo  like $P{pv_pel_codigo}
AND     pos.est_codigo  IN (SELECT est_codigo e
                            FROM   org.org_estructura e
                            WHERE  e.est_nivel  = 4
                            START WITH e.est_codigo = $P{pn_est_codigo}
                            CONNECT BY PRIOR e.est_codigo = e.est_codigo_padre)
AND     cal.aud_eliminado = 'N'
AND     cal.cal_retirado  = 'N'
AND     mat.aud_eliminado = 'N'
AND     mat.mat_anulado   = 'N'
AND     inc.ins_codigo    = mat.ins_codigo
AND     oii.oii_codigo    = inc.oii_codigo
AND     mod.mod_codigo    = oii.mod_codigo
AND     oii.mod_codigo    like $P{pv_mod_codigo}
AND     inc.ins_codigo  NOT  IN  (select rei.ins_codigo  --SIN DECLARATORIA DE APTITUD
                                  from sse.sse_tipo_resolucion tir,
                                       sse.sse_tip_con_tip_res tct,
                                       sse.sse_antecedente     ant,
                                       sse.sse_resolucion      res,
                                       sse.sse_resolucion_inscripcion rei
                                  where tir.tir_codigo = tct.tir_codigo
                                  and   tct.tct_codigo = ant.tct_codigo
                                  and   ant.ant_codigo = res.ant_codigo
                                  and   tir.tir_codigo  = 9
                                  and   res.aud_eliminado = 'N'
                                  and   res.res_estado  = 'A'
                                  and   res.res_codigo   = rei.res_codigo)

AND (dis.dis_fecha_fin) IN     (select distinct max(dist.dis_fecha_fin)   --FECHA FIN ULTIMO MODULO
                                                from ofe.ofe_grupo grup,
                                                     ofe.ofe_distributivo dist,
                                                     ped.ped_malla        mal,
                                                     ped.ped_proyecto_educacion pro,
                                                     ped.ped_tipo_proyecto_educacion      tpe,
                                                     ped.ped_materia                      mte,
                                                     ped.ped_des_est_pro_edu              dep,
                                                     org.org_descripcion_estructura       dca,
                                                     org.org_estructura                   posg,
                                                     org.org_estructura                   cam,
                                                     ped.ped_mod_pro_edu                  mpe,
                                                     ped.ped_est_mod_pro_edu              emp
                                                where grup.gru_codigo = dist.gru_codigo
                                                and   grup.mal_codigo = mal.mal_codigo
                                                and   pro.pre_numero = mal.pre_numero
                                                and   pro.tpe_codigo = tpe.tpe_codigo
                                                and   tpe.ctp_codigo = 4--Posgrados
                                                and   mte.mat_codigo = mal.mat_codigo
                                                and   pro.pre_numero = dep.pre_numero
                                                and   dca.dee_codigo = dep.dee_codigo
                                                and   dca.dee_codigo = posg.dee_codigo
                                                and   posg.est_codigo_padre = cam.est_codigo
                                                and   pro.pre_numero = pre.pre_numero
                                                and   grup.pel_codigo = gru.pel_codigo
                                                and   posg.est_codigo = pos.est_codigo
                                                and   pro.pre_numero = mpe.pre_numero
                                                and   mpe.mpe_codigo = emp.mpe_codigo
                                                and   emp.emp_codigo = grup.emp_codigo
                                                and   grup.gru_item  = gru.gru_item
                                                and   grup.aud_eliminado = 'N'
                                                and   posg.est_codigo_padre = emp.est_codigo
                                                and   mal.mal_nivel    = (select max(m.mal_nivel)--NIVEL MAS ALTO DE LA MALLA
                                                                          from ped.ped_malla m
                                                                          where m.pre_numero = pro.pre_numero
                                                                          and   m.aud_eliminado = 'N'
                                                                          ))
AND  ADD_MONTHS(TO_DATE(TO_CHAR(dis.dis_fecha_fin,'dd,mm,yyyy'),'DD/MM/YYYY'),6)  >= SYSDATE
ORDER   BY sede,campus,posgrado,pel_codigo,modalidad,estudiante]]>
	</queryString>
